#include<iostream>
using namespace std;
int main(){
cout<<"5\n";
cout<<"3 3\n";
cout<<"-1 0 6\n";

cout<<"9 2\n";
cout<<"-1 0 1 2 3 4 5 6 7\n";

cout<<"5 4\n";
cout<<"-5 0 5 8 7\n";

cout<<"8 4\n";
cout<<"-3 0 -7 -9 7 66 999 89\n";

cout<<"4 4\n";
cout<<"-1 0 8 90\n";
}
